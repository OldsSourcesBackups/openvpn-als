<?xml version="1.0" encoding="UTF-8"?>
<project name="adito-application-putty-portable-telnet" default="dist">

        <!-- Standard build locations -->
        <property name="build.src" value="src" />
        <property name="output.file" value="adito-application-putty-portable-telnet.zip" />

	<!-- Build the adito-application-putty-portable-telnet.zip -->
	<target name="dist">
		<zip destfile="${output.file}">
			<zipfileset dir="${build.src}" prefix="adito-application-putty-portable-telnet">
			</zipfileset>
		</zip>
	</target>

</project>
